{
  "name": "admin-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.7",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "typescript": "3.5.1"
  },
  "scripts": {
    "build:css": "node-sass src/styles/sass/ -o src/styles/css/",
    "watch:css": "npm run build:css && node-sass src/styles/sass/ -o src/styles/css/ --watch --recursive",
    "start:js": "react-scripts start",
    "start": "npm-run-all -p watch:css start:js",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fmt": "prettier --print-width 80 --no-bracket-spacing --single-quote --trailing-comma all --write src/**/*.ts{,x}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2"
  }
}
